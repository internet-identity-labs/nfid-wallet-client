{
  "name": "nfid-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nfid-demo/src",
  "projectType": "application",
  "tags": ["scope:app", "type:application", "layer:app"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/nfid-demo"],
      "options": {
        "command": "BUILD_PATH=../../dist/apps/nfid-demo npx craco build",
        "cwd": "apps/nfid-demo"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/nfid-demo/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/nfid-demo/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
