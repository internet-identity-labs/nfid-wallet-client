{
  "name": "nfid-demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nfid-demo/src",
  "projectType": "application",
  "tags": ["scope:app", "type:application", "layer:app"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "web",
        "compiler": "babel",
        "outputPath": "dist/apps/nfid-demo",
        "index": "apps/nfid-demo/src/index.html",
        "baseHref": "/",
        "main": "apps/nfid-demo/src/index.tsx",
        "tsConfig": "apps/nfid-demo/tsconfig.app.json",
        "assets": [
          "apps/nfid-demo/src/favicon.ico",
          "apps/nfid-demo/src/assets"
        ],
        "styles": ["apps/nfid-demo/src/index.scss"],
        "isolatedConfig": true,
        "webpackConfig": "apps/nfid-demo/craco.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/nfid-demo/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/nfid-demo/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
