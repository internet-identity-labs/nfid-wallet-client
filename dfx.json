{
  "canisters": {
    "nfid-wallet-client": {
      "frontend": {
        "entrypoint": "dist/apps/nfid-frontend/index.html"
      },
      "source": [
        "dist/apps/nfid-frontend/"
      ],
      "type": "assets"
    },
    "nfid-demo": {
      "frontend": {
        "entrypoint": "dist/apps/nfid-demo/index.html"
      },
      "source": [
        "dist/apps/nfid-demo/"
      ],
      "type": "assets"
    },
    "nfid-storybook-frontend": {
      "frontend": {
        "entrypoint": "dist/apps/nfid-demo/index.html"
      },
      "source": [
        "dist/storybook/nfid-frontend/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    },
    "dev": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "stage": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}