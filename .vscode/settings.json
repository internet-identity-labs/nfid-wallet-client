{
  "liveshare.launcherClient": "visualStudioCode",
  "cucumberautocomplete.steps": ["./apps/nfid-frontend-e2e/src/steps/*.ts"],
  "cucumberautocomplete.syncfeatures": "./apps/nfid-frontend-e2e/src/features/**/*.feature",
  "cucumberautocomplete.strictGherkinCompletion": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.d.ts, ${capture}.types.ts, ${capture}.js, ${capture}.typegen.ts, ${capture}.spec.ts, ${capture}.spec.tsx, ${capture}.mocks.ts",
    "*.tsx": "${capture}.ts, ${capture}.typegen.ts, ${capture}.spec.ts, ${capture}.spec.tsx, ${capture}.stories.tsx, ${capture}.md, ${capture}.mocks.ts"
  },
  "explorer.fileNesting.expand": true,
  "explorer.fileNesting.enabled": true,
  "cSpell.words": ["INFT", "wagmi"],
  "jest.nodeEnv": {
    "experimental-vm-modules": true
  },
  "jest.autoRun": {
    "watch": false,
    "onSave": "test-file",
    "onStartup": ["all-tests"]
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[feature]": {
    "editor.defaultFormatter": "alexkrechik.cucumberautocomplete"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  }
}
