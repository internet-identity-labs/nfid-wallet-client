{
  "name": "@internet-identity-labs/nfid-frontend",
  "version": "0.0.0",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build-storybook": "build-storybook -s public",
    "build:example": "export EXAMPLE_BUILD=1 && craco build",
    "build": "craco build",
    "clear": "rm -rf node_modules build allure-results",
    "dev": "craco start",
    "eject": "craco eject",
    "format:examples": "prettier --write ./examples/MultipassDemo/src",
    "format:check": "prettier --check ./src",
    "format": "prettier --write ./src",
    "generate-cucumber-html-report": "ts-node ./test/reporter/index.ts",
    "generate:js": "../../Dfinity/candid/target/debug/didc bind ../identity-manager/src/identity_manager/identity_manager.did -t js > frontend/generated/identity_manager.js",
    "generate:types": "../../Dfinity/candid/target/debug/didc bind ../identity-manager/src/identity_manager/identity_manager.did -t ts > frontend/generated/identity_manager.d.ts",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "test:dev": "jest --passWithNoTests --config=jest.config.js --detectOpenHandles --watch",
    "test:dev:silent": "yarn test:dev --silent=true",
    "test:e2e": "wdio run wdio.conf.ts",
    "test:e2e:desktop": "wdio run wdio.conf.ts",
    "test:e2e:mobile": "wdio run wdio.mobile.conf.ts",
    "test:e2e:bs:mobile": "wdio run wdio.bs.mobile.conf.ts",
    "test:e2e:bs:desktop": "wdio run wdio.bs.desktop.conf.ts",
    "test:e2e:debug": "cross-env DEBUG=true wdio run wdio.conf.ts --inspect",
    "test": "jest --passWithNoTests --config=jest.config.js --detectOpenHandles --silent=true",
    "tunnel": "./scripts/tunnel.sh"
  },
  "cracoConfig": "craco.config.ts",
